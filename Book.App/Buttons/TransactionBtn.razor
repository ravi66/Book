@if (IsIcon)
{
    <MudIconButton Icon="@Icon" Variant="@Variant" Color="Color.Primary" Size="Size.Small" OnClick="CallTransactionDialog" Title="@AriaLabel" />
}
else
{
    <MudButton OnClick="CallTransactionDialog" StartIcon="@Icon" Color="Color.Inherit" Variant="Variant.Outlined" >@Localizer["NewEntry"]</MudButton>
}